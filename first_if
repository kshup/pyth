not1= float(input('1. notu giriniz'))
not2=float(input('2, notu giriniz'))
not3= float(input('3. notu giriniz'))

ortalama = (not1+not2+not3)/3

if (ortalama<49.9):
        sonuc='kaldi'
elif (ortalama<50 and 59.9<ortalama):
     sonuc='gecti'
elif (ortalama>60 and ortalama<70):
     sonuc='orta'
elif (ortalama<71 and ortalama>90):
     sonuc='iyi'
elif (ortalama>91):
     sonuc='cok iyi'
print(sonuc)
